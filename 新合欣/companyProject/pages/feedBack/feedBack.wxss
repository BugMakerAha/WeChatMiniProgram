/* pages/feedBack/feedBack.wxss */
.bianjiao{
  width: 6vw;
  height: 6vw;
  position: absolute;
}
page {
  display: flex;
  flex-direction: column;
  background-color: rgb(239, 239, 239);
  height: 100%;
}

.contain {
  flex: 1;
  
  font-size: 3.5vw;
}

.content {
  width: 100vw;
  height: 50vw;
  padding: 3vw;
  background-color: white;
  border-radius: 5px;
}

.phone {
  margin-top: 3vw;
  padding: 2vw 3vw;
  width: 100vw;
  background-color: white;
  border-radius: 5px;
}

.photo_contain {
  display: flex;
  flex-direction: row;
  flex-wrap: wrap;
  margin: 0 2vw;

}

.photo_item {
  width: 31%;
  height: 20vw;
  margin: 3vw 1.16% 0 1.16%;
  background-color: white;
}

.photo_item_img {
  width: 100%;
  height: 100%;
}

.close {
  position: absolute;
  width: 6vw;
  height: 6vw;
  margin-top: -2vw;
  margin-left: 25%;
  
}
