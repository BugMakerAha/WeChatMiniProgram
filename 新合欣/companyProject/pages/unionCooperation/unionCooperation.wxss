/* pages/creativeCandidate/creativeCandidate.wxss */
page{
  height: 100%;
  display: flex;
  flex-direction: column
}
.contain{
  display: flex;
  flex-direction: column;
  flex: 1;
}
.dir_row{
  display: flex;
  flex-direction: row;
}
.dir_column{
  display: flex;
  flex-direction: column;
}
.useritem{
  display: flex;
  flex-direction: row;
  align-items: center;
  width: 92vw;
  margin-left: 2vw;
  border-bottom: 1px solid green;
  height: 20vw;
}
.usericon{
  background-color: green;
  width: 12vw;
  height: 12vw;
  border-radius: 50%;
  box-shadow: 0 5px 4px -2px gray;
  
}